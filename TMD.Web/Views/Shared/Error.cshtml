@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    Layout = null;
}
<div class="mainbar">
    <div class="page-head">
        <div class="container">
            <hgroup>
                <h1 class="text-error">Error.</h1>
                <h2 class="text-error">An error occurred while processing your request.</h2>
            </hgroup>
            <p>
                @ViewBag.Error
            </p>
        </div>
    </div>
</div>
