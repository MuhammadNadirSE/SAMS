<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Bootstrap Admin App + jQuery">
    <meta name="keywords" content="app, responsive, jquery, bootstrap, dashboard, admin">
    <title>InnoSTARK Technologies - @ViewBag.Title</title>
    <!-- =============== VENDOR STYLES ===============-->
    
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Vendor/datatables/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Vendor/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/Scripts/app/modules/clear-storage.js"></script>
    <script src="~/Scripts/app/modules/fullscreen.js"></script>
    <script src="~/Scripts/app/modules/load-css.js"></script>
    <script src="~/Scripts/app/modules/maps-vector.js"></script>
    <script src="~/Scripts/app/modules/notify.js"></script>
    <script src="~/Scripts/app/modules/navbar-search.js"></script>
    <script src="~/Scripts/app/modules/panel-tools.js"></script>
    <script src="~/Scripts/app/modules/play-animation.js"></script>
    <script src="~/Scripts/app/modules/toggle-state.js"></script>
    <script src="~/Scripts/app/modules/utils.js"></script>
    <script src="~/Scripts/demo/demo-datatable.js"></script>
    <script src="~/Scripts/demo/demo-flot.js"></script>
    <script src="~/Scripts/demo/demo-forms.js"></script>
    <script src="~/Scripts/demo/demo-panels.js"></script>
    <script src="~/Scripts/demo/demo-rtl.js"></script>


    @Styles.Render("~/bundles/datetimePickerCss")

    @Scripts.Render("~/bundles/datatables")
    @Scripts.Render("~/bundles/demoDatatable")
    @Styles.Render("~/bundles/fontawesome")
    @Styles.Render("~/bundles/simpleLineIcons")
    @Styles.Render("~/bundles/animatecss")
    @Styles.Render("~/bundles/whirl")

    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/datetimePicker")

    <!-- =============== PAGE VENDOR STYLES =============== -->
    @if (IsSectionDefined("Styles"))
        {@RenderSection("Styles", required: false)}
    <!-- =============== BOOTSTRAP STYLES ===============-->
    <link href="@Styles.Url("~/Content/app/css/bootstrap.css")" rel="stylesheet" type="text/css" id="bscss" />
    <!-- =============== APP STYLES ===============-->
    <link href="@Styles.Url("~/Content/app/css/app.css")" rel="stylesheet" type="text/css" id="maincss" />
         <link href="@Styles.Url("~/Content/mvc-override.css")" rel="stylesheet" type="text/css"/>
    @Styles.Render("~/bundles/datatablesCss")
    <link href="~/Content/styleIST.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <!-- top navbar-->
        <header class="topnavbar-wrapper">
            @Html.Partial("_TopNavbar")
        </header>
        <!-- sidebar-->
        <aside class="aside">
            @Html.Partial("_Sidebar")
        </aside>
        <!-- offsidebar-->
        <aside class="offsidebar hide">
            @Html.Partial("_Offsidebar")
        </aside>
        <!-- Main section-->
        <section>
            <!-- Page content-->
            <div class="content-wrapper">
                @RenderBody()
            </div>
        </section>
        <!-- Page footer-->
        <footer>
            @Html.Partial("_Footer")
        </footer>
    </div>
    @if (IsSectionDefined("BodyArea"))
        {@RenderSection("BodyArea", required: false)}
    <!-- =============== VENDOR SCRIPTS ===============-->
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/matchMedia")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/storage")
    @Scripts.Render("~/bundles/jqueryEasing")
    @Scripts.Render("~/bundles/animo")
    @Scripts.Render("~/bundles/slimscroll")
    @Scripts.Render("~/bundles/screenfull")
    @Scripts.Render("~/bundles/localize")
    @Scripts.Render("~/bundles/demoRTL")
    <!-- =============== PAGE VENDOR SCRIPTS ===============-->
    @RenderSection("scripts", required: false)
    <!-- =============== APP SCRIPTS ===============-->
    @Scripts.Render("~/bundles/Angle")
</body>
</html>